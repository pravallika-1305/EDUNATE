{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "edunate": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/edunate",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/styles/contact_responsive.css",
              "src/assets/styles/bootstrap4/bootstrap.min.css",
              "src/assets/styles/courses_responsive.css",
              "src/assets/styles/courses_styles.css",
              "src/assets/styles/elements_responsive.css",
              "src/assets/styles/elements_styles.css",
              "src/assets/styles/main_styles.css",
              "src/assets/styles/news_post_responsive.css",
              "src/assets/styles/news_post_styles.css",
              "src/assets/styles/news_responsive.css",
              "src/assets/styles/news_styles.css",
              "src/assets/styles/responsive.css",
              "src/assets/styles/teachers_responsive.css",
              "src/assets/styles/teachers_styles.css",
              "src/assets/plugins/OwlCarousel2-2.2.1/animate.css",
              "src/assets/plugins/OwlCarousel2-2.2.1/owl.carousel.css",
              "src/assets/plugins/OwlCarousel2-2.2.1/owl.theme.default.css",
              "src/assets/plugins/fontawesome-free-5.0.1/css/fontawesome-all.css"
            ],
            "scripts": ["src/assets/js/jquery-3.2.1.min.js",
              "src/assets/styles/bootstrap4/popper.js",             
              "src/assets/styles/bootstrap4/bootstrap.min.js",
              
              "src/assets/js/custom.js",
              "src/assets/plugins/scrollmagic/ScrollMagic.min.js",
              "src/assets/plugins/OwlCarousel2-2.2.1/owl.carousel.js",
              "src/assets/plugins/greensock/animation.gsap.min.js",
              "src/assets/plugins/greensock/ScrollToPlugin.min.js",
              "src/assets/plugins/greensock/TimelineMax.min.js",
              "src/assets/plugins/greensock/TweenMax.min.js",
              "src/assets/plugins/parallax.js-2.0.0/jquery.parallax.min.js",
              "src/assets/plugins/progressbar/progressbar.min.js",
              "src/assets/plugins/easing/easing.js",
              "src/assets/js/contact_custom.js",
              "src/assets/js/courses_custom.js",
              "src/assets/js/teachers_custom.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "edunate:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "edunate:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "edunate:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "edunate:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "edunate:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "edunate",
  "cli": {
    "analytics": "d77c2ac7-767a-4fc9-b1fc-1ad663621532"
  }
}